{
  "name": "htr3n/php-project-template",
  "description": "A basic PSR-4 project skeleton for PHP development",
  "keywords": [
    "php",
    "project",
    "psr4",
    "phpunit",
    "testing",
    "skeleton",
    "template"
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Huy Tran",
      "email": "hoang.huy.tran@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/App/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests/App/"
    }
  },
  "minimum-stability": "stable"
}
